    </bean>
    
    <bean id="xjmsContainer0" class="org.springframework.data.jdbc.jms.listener.oracle.XmlMessageListenerContainer" parent="dmlcxml">
        <property name="destinationName" value="DEMO_XQUEUE0"/>
    </bean>
     
    <bean id="xjmsContainer1" class="org.springframework.data.jdbc.jms.listener.oracle.XmlMessageListenerContainer" parent="dmlcxml">
