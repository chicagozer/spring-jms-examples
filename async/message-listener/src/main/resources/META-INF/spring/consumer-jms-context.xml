    </bean>
    
    <bean id="jmsContainer6" class="org.springframework.jms.listener.DefaultMessageListenerContainer" parent="dmlcsimple">
        <property name="destinationName" value="DEMO_QUEUE6"/>
        
    </bean>
    
    <bean id="jmsContainer7" class="org.springframework.jms.listener.DefaultMessageListenerContainer" parent="dmlcsimple">
        <property name="destinationName" value="DEMO_QUEUE7"/>
        
    </bean>
    <bean id="jmsContainer8" class="org.springframework.jms.listener.DefaultMessageListenerContainer" parent="dmlcsimple">
