    <bean id="dmlc" abstract="true">
        <property name="connectionFactory" ref="connectionFactory"/>
        <property name="sessionTransacted" value="true"/>
        <property name="concurrentConsumers" value="1"/>
        <property name="maxConcurrentConsumers" value="2"/>
    </bean>
     
    <bean id="dmlcsimple" abstract="true" parent="dmlc">
        <property name="messageListener" ref="messageListener" />
    </bean>
     
    <bean id="dmlcxml" abstract="true" parent="dmlc">
        <property name="messageListener" ref="xmlMessageListener" />
    </bean>

   
    
  <bean id="jmsContainer1" class="org.springframework.jms.listener.DefaultMessageListenerContainer" parent="dmlcsimple">
